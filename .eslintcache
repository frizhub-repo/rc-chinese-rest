tate,\n        productByCategory: state.productByCategory.concat(actions.payload),\n      };\n    default:\n      return state;\n  }\n}\n","/mnt/e/frizhub/rc-chinese-rest/src/reducers/orderReducer.js",["295","296"],"const initialState = {\n  total: 0,\n  items: [],\n  minimum: 0,\n  delivery: 0,\n};\n\nexport default function (state = initialState, action) {\n  switch (action.type) {\n    case \"ADD_ITEM\":\n      const index1 = state.items\n        .map(function (e) {\n          return e.product;\n        })\n        .indexOf(action.payload.product);\n      if (index1 !== -1) {\n        const items = state.items;\n        items[index1].quantity =\n          parseInt(items[index1].quantity) + parseInt(action.payload.quantity);\n        return { ...state, items };\n      } else {\n        return {\n          ...state,\n          items: state.items.concat(action.payload),\n        };\n      }\n    //   return {\n    //     ...state,\n    //     items: state.items.concat(action.payload),\n    //   };\n\n    case \"TOTAL\":\n      return {\n        ...state,\n        total: state.total + action.payload,\n      };\n    case \"REMOVE_ITEM\":\n      return {\n        ...state,\n        items: state.items.filter((itm) => itm.name != action.payload.name),\n        total: state.total - action.payload.price * action.payload.quantity,\n      };\n    case \"REMOVE_ORDER_ITEMS\":\n      return {\n        ...state,\n        items: [],\n        total: 0,\n      };\n    case \"ADD_QUANTITY\":\n      const product = state.items.map((item) => {\n        if (item.product === action.payload.product) {\n          state.total += item.price;\n          return { ...item, quantity: item.quantity + 1 };\n        } else {\n          return item;\n        }\n      });\n      return {\n        ...state,\n        items: product,\n      };\n    case \"REMOVE_QUANTITY\":\n      const products = state.items.map((item) => {\n        if (item.product === action.payload.product) {\n          state.total -= item.price;\n          return { ...item, quantity: item.quantity - 1 };\n        } else {\n          return item;\n        }\n      });\n      return {\n        ...state,\n        items: products,\n      };\n    default:\n      return state;\n  }\n}\n","/mnt/e/frizhub/rc-chinese-rest/src/components/Menu/Body.js",["297","298","299","300","301","302","303","304","305","306","307","308"],"import React, { useState, useEffect } from \"react\";\nimport select from \"../../images/triangle.svg\";\nimport Skeleton from \"@material-ui/lab/Skeleton\";\nimport { productsList } from \"../../api/products\";\n\nimport Table from \"./Table\";\n\nfunction Body() {\n  const [products, setProducts] = useState([]);\n  const [key, setKey] = useState(1);\n  const [loading, setLoading] = useState(false);\n  const fetchProducts = async () => {\n    try {\n      setLoading(true);\n      const res = await productsList();\n      setProducts(res?.data);\n      setLoading(false);\n    } catch (error) {\n      setLoading(false);\n      console.log(error);\n    }\n  };\n  useEffect(() => {\n    fetchProducts();\n  }, []);\n  return (\n    <div className=\"w-full py-4 px-36 \">\n      <div className=\"flex justify-content-center mb-0\">\n        <div className=\"mx-4 cursor-pointer\" onClick={() => setKey(1)}>\n          <p\n            className={`text-center font-weight-bold text-xs mt-2 mb-1 ${\n              key == 1 ? \"text-yellow-400\" : \"\"\n            }`}\n          >\n            Breakfast\n          </p>\n          {key === 1 && (\n            <div className=\"h-4 w-full\">\n              <img src={select} className=\"w-4 h-full  object-center \" />\n            </div>\n          )}\n        </div>\n        <div className=\"mx-4 cursor-pointer\" onClick={() => setKey(2)}>\n          <p\n            className={`text-center font-weight-bold text-xs mt-2 mb-1 ${\n              key == 2 ? \"text-yellow-400\" : \"\"\n            }`}\n          >\n            Lunch\n          </p>\n   